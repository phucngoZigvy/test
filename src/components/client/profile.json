{
  "collectionName": "components_user_profile_profiles",
  "info": {
    "displayName": "Profile",
    "icon": "address-card",
    "description": ""
  },
  "options": {},
  "attributes": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "birthday": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female"
      ]
    },
    "organization": {
      "type": "string"
    },
    "website": {
      "type": "string",
      "regex": "[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)"
    },
    "bio": {
      "type": "text"
    },
    "avatar": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
